{
  "name": "my-diary",
  "version": "1.0.0",
  "description": "allows user to create, view and modify diary entries",
  "main": "server.js",
  "scripts": {
    "test": "set NODE_ENV=test && nyc --reporter=html --reporter=text -r ./node_modules/dotenv/config models mocha --require babel-core/register ./test --recursive --exit",
    "start": "npm run migrate && babel-node -r ./node_modules/dotenv/config server.js --exec babel-node --presets es2015,stage-2",
    "serve": "babel-node server.js --exec babel-node --presets es2015,stage-2",
    "test2": "npm run migrate && nyc mocha",
    "db-init": "createdb && psql -f diaries.sql",
    "migrate": "node -r babel-register -r ./node_modules/dotenv/config models && npm run serve",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Obuladike Chisom",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jasmine-es6": "^0.4.3",
    "joi": "^13.4.0",
    "mocha": "^5.2.0",
    "pg": "^7.4.3",
    "pg-promise": "^5.9.7",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "istanbul": "0.3.5",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jscoverage": "^0.6.0",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.3",
    "nodeunit": "^0.11.3",
    "nyc": "^12.0.2",
    "should": "^13.2.1"
  }
}
